   "name": "Leave Application-signatures", 
   "no_copy": 0, 
   "options": "{% if doc.workflow_state == 'Approved' %}\n\n{% set issuer = frappe.get_all('User', fields=['full_name','signature'], \nfilters = {'email':doc.owner})%} \n\n\n{% set hr_user = frappe.get_all('Communication', fields=['sender_full_name'],filters = {'reference_name':doc.name,'reference_doctype':doc.doctype,'subject':'Pending for General Manager'})%}\n{% if (hr_user is defined) and hr_user %}\n{% set hr_user_sign = frappe.get_all('User', fields=['signature'],filters = {'full_name':hr_user[0].sender_full_name})%}\n{% endif %}\n\n\n{% set hr_manager = frappe.get_all('Communication', fields=['sender_full_name'],filters = {'reference_name':doc.name,'reference_doctype':doc.doctype,'subject':'Approved'})%}\n{% if (hr_manager is defined) and hr_manager %}\n{% set hr_manager_sign = frappe.get_all('User', fields=['signature'],filters = {'full_name':hr_manager[0].sender_full_name})%}\n{% endif %}\n<table class=\"table table-bordered table-condensed\" id=\"sign\">\n<thead>\n<tr>\n<th class=\"table-sr\" style=\"width: 150px; vertical-align: middle !important;\">{{_('Approved by')}}</th>\n<th style=\"width: 150px; vertical-align: middle !important;\">\n{{_('Name')}}</th>\n<th style=\"width: 150px; vertical-align: middle !important; text-align: center;\">\n{{_('Signature')}}</th>\n</tr>\n</thead>\n<tbody><tr>\n<td class=\"table-sr\" style=\"vertical-align: middle !important;\">\n<strong>{{_('Line Manager')}}<strong></strong></strong></td>\n<td style=\"vertical-align: middle !important;\">\n<div class=\"value\">\n{{_(issuer[0].full_name) or '' }}\n</div>\n</td>\n<td style=\"vertical-align: middle !important; text-align: center;\">\n<div class=\"value\">\n<img src=\"{{issuer[0].signature}}\" style=\"width: 45% !important;\">\n</div>\n</td>\n</tr><tr>\n<td class=\"table-sr\" style=\"vertical-align: middle !important;\">\n<strong>{{_('HR Manager')}}<strong></strong></strong></td>\n<td style=\"vertical-align: middle !important;\">\n<div class=\"value\">\n{{_(hr_user[0].sender_full_name) or '' }}\n</div>\n</td>\n<td style=\"vertical-align: middle !important; text-align: center;\">\n<div class=\"value\">\n<img src=\"{{hr_user_sign[0].signature}}\" style=\"width: 45% !important;\">\n</div>\n</td>\n</tr><tr>\n<td class=\"table-sr\" style=\"vertical-align: middle !important;\">\n<strong>{{_('General Manager')}}<strong></strong></strong></td>\n<td style=\"vertical-align: middle !important;\">\n<div class=\"value\">\n{{_(hr_manager[0].sender_full_name) or '' }}\n</div>\n</td>\n<td style=\"vertical-align: middle !important; text-align: center;\">\n<div class=\"value\">\n<img src=\"{{hr_manager_sign[0].signature}}\" style=\"width: 45% !important;\">\n</div>\n</td>\n</tr>\n\n\n</tbody>\n</table>\n{% endif %}\n", 
